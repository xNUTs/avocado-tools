server
	temp				out/temp
	port 				1026
	auto_index 	true
	#gzip				false
	#agzip				false
	#print_log	 	true
	defaults		index.html index.htm default.html
	max_file_size 				1073741824 # 1024 * 1024 * 1024
	max_form_data_size		52428800 # 50 * 1024 * 1024
	max_upload_file_size 	52428800 # 50 * 1024 * 1024
	#expires	0
	router
		,
			match			/$console/{action}/{args}
			service		Console
		,
			match			/$file/{action}/{path}
			service		File
		,
			match			/{service}/{action}
		, 
			match			/{path}
			service		File
			action		unknown
